#
# Inventory file generated by fabric2hosts.j2 using infr_data_model.yml as source information.
# Node names must include Spine or Leaf to be included in the host inventory.
#


[SPINE]
{% for node in nodes %}
{% if "Spine" in node.name %}
{{ node.name }} ansible_host={{ node.mgmt }} ansible_os={{node.os}}
{% endif %}
{% endfor %}

[LEAF]
{% for node in nodes %}
{% if "Leaf" in node.name %}
{{ node.name }} ansible_host={{ node.mgmt }} ansible_os={{node.os}}
{% endif %}
{% endfor %}
